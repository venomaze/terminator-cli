{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "node/shebang": "off",
    "consistent-return": "off",
    "no-await-in-loop": "off",
    "radix": "off",
    "no-plusplus": "off"
  }
}
